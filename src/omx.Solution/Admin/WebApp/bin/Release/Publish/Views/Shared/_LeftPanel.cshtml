@if (ViewBag.AppSidebar !=null && this.Request.IsAuthenticated)
{
	<aside class="page-sidebar">
		@Html.Partial("_Logo") 
		<nav id="js-primary-nav" class="primary-nav" role="navigation">

			@if (Request.IsAuthenticated)
			{
				Html.RenderAction("RenderMenus", "RoleMenus");
			}
			@Html.Partial("_NavFilterMsg")

		</nav>
		@Html.Partial("_NavFooter")
	</aside>
}